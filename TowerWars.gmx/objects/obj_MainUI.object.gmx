if other.uiControlName == 'upgradeStore'
{
    if(current_second-t=0){//If the time has not changed
        global.purchase = other.items[other.itemindex];
        show_message(other.itemindex);
    }else{
        t=current_second;//If the time has changed get the second
    }
    
}
else if other.uiControlName == 'gotoLevel'
{
    if(current_second-t=0){//If the time has not changed
        i = other.itemindex
        
        switch i
        {
            case 0: room_goto(rm_skirmishLV0);
            case 1: room_goto(rm_skirmishLV0);
            case 2: room_goto(rm_skirmishLV0);
        }
        
    }else{
        t=current_second;//If the time has changed get the second
    }
    
}
else if other.uiControlName == 'upgrade'
{
    if (ui_upgradeStore.visible == true)
    {
        ui_upgradeStore.visible = false;
        ui_purchaseButton.visible = false;
    }
    else
    {
        ui_upgradeStore.visible = true;
        ui_purchaseButton.visible = true;
    }
}
else if other.uiControlName == 'purchase'
{
